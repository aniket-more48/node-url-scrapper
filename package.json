{
  "name": "web_url_scraper",
  "version": "1.0.0",
  "description": "Backend server for Url meta data scrapping",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "yarn clean && tsc",
    "prestart": "yarn build",
    "start": "tsc --watch & nodemon dist/index.js",
    "test": "jest --testTimeout=10000",
    "pre-commit": "yarn tslint && yarn build",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "dependencies": {
    "apollo-server": "^2.9.6",
    "apollo-server-express": "^2.9.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.13",
    "open-graph-scraper": "^4.7.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.5.41",
    "@types/node": "^13.1.7",
    "husky": "^4.2.0",
    "jest": "^26.6.3",
    "nodemon": "^1.19.2",
    "ts-jest": "^26.4.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  }
}
